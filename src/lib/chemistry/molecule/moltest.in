% Emacs should use -*- keyval -*- mode.

atominfo<AtomInfo>: (
    vdw_radius: (
      scale_factor = 1.2
      unit = angstrom
      H = 1.3
      Li = 0.1818
      )
    bragg_radius: (
      unit = angstrom
      H = 0.888
      )
    atomic_radius: (
      unit = angstrom
      H = 0.999
      )
    rgb: (
      H = [ 0.0 0.0 1.0 ]
      He:2 = 0.5
      )
    mass: (
      unit = amu
      H = 2.1
      )
  )

energy<TaylorMolecularEnergy>: (
  molecule<Molecule>: (
      symmetry = c2v
      { atom_labels atoms geometry } = {
            O        O     [0.0  0.0 0.0]
            H1       H     [0.0  1.0 1.0]
            H2       H     [0.0 -1.0 1.0]
        }
    ) 
  coordinates<SetIntCoor>: [
       <StreSimpleCo>: ( atoms = [ 1 2 ] label = "r(OH1)" )
       <StreSimpleCo>: ( atoms = [ 1 3 ] label = "r(OH2)" )
       <BendSimpleCo>: ( atoms = [ 2 1 3 ] label = "theta(HOH)" )
    ]
%  force_constants: [
%      [ 1 1 4.0 ]
%      [ 2 2 4.0 ]
%      [ 3 3 2.0 ]
%      [ 1 2 1.5 ]
%      [ 1 3 0.5 ]
%      [ 2 3 0.5 ]
%    ]
  )

grad<FinDispMolecularGradient>: (
    energy = $:energy
  )

% TaylorMolecularEnergy does work with FinDispMolecularHessian
hess<FinDispMolecularHessian>: (
    energy = $:energy
  )

freq<MolecularFrequencies>: (
    molecule = $:molecule
  )

